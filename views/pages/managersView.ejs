                        Ученики 1 класса
                      </button>
                    </h2>
                    <div id="collapseOne" class="accordion-collapse collapse" aria-labelledby="headingOne" data-bs-parent="#accordionExample">
                      <div class="accordion-body">

                          <% students.filter((student) => student.class_number == 1).forEach(function(student) { %>
                                <a class="btn btn-secondary btn-sm m-1" href=<%= `${globalLink}/views/${student.student_id}` %>>
                                  <strong><%= student.name %></strong>
